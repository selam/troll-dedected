{
  "manifest_version": 2,
  "name": "Ekşi duyuru için troll dedected ",
  "description": "Ekşi duyuru troll dedected eklentisi, çok teknolojik.",
  "version": "0.3",
  "author": "Timu Eren<selamtux@gmail.com>",
  "homepage_url": "https://github.com/selam/troll-dedected",
  "permissions": [
    "tabs",
    "http://www.eksiduyuru.com/",
    "https://www.eksiduyuru.com/"	
  ],
  "icons": {
   },
  "web_accessible_resources": [
    "script/troll-blocker.js",
    "css/troll-blocker.css",
    "images/check-circle-outline-16.png",
    "images/block-16.png"
  ],
  "homepage_url": "https://github.com/selam/troll-blocker",
  "content_scripts": [ {
      "js": [ "insertation.js" ],
      "matches": [ "http://www.eksiduyuru.com/*", "https://www.eksiduyuru.com/*"],
      "run_at": "document_end"
   } ]
}
